{
  "name": "suf-gen",
  "version": "0.1.2",
  "main": "./dist/cli.js",
  "scripts": {
    "prepack": "jest && del ./dist && npm run build && suf",
    "link:cli": "yarn && yarn build && npm link",
    "test": "jest --coverage",
    "test:watch": "jest --watch --coverage",
    "start": "yarn watch",
    "watch": "tsc -w",
    "build": "tsc -b"
  },
  "author": "Syler",
  "license": "MIT",
  "bin": {
    "suf-gen": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/TheRealSyler/suf-gen"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.12.54",
    "suf-cli": "^0.1.1",
    "jest": "^26.6.1",
    "ts-jest": "^26.4.2",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@sorg/log": "^2.2.0",
    "fs-extra": "^9.0.1",
    "suf-node": "^1.2.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx",
      "node"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/logger.ts",
      "/node_modules/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
